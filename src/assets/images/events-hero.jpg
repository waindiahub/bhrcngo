<svg width="1200" height="400" viewBox="0 0 1200 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="eventsGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e83e8c;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#6f42c1;stop-opacity:0.7" />
      <stop offset="100%" style="stop-color:#007bff;stop-opacity:0.9" />
    </linearGradient>
    <pattern id="eventsPattern" patternUnits="userSpaceOnUse" width="80" height="80">
      <circle cx="40" cy="40" r="3" fill="white" opacity="0.1"/>
      <circle cx="20" cy="20" r="2" fill="white" opacity="0.08"/>
      <circle cx="60" cy="20" r="2" fill="white" opacity="0.08"/>
      <circle cx="20" cy="60" r="2" fill="white" opacity="0.08"/>
      <circle cx="60" cy="60" r="2" fill="white" opacity="0.08"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="400" fill="url(#eventsGradient)"/>
  <rect width="1200" height="400" fill="url(#eventsPattern)"/>
  
  <!-- Calendar Icon -->
  <g transform="translate(200, 100)">
    <!-- Calendar Base -->
    <rect x="0" y="20" width="120" height="140" rx="10" fill="white" opacity="0.3"/>
    <rect x="0" y="20" width="120" height="40" rx="10" fill="white" opacity="0.5"/>
    
    <!-- Calendar Rings -->
    <circle cx="25" cy="10" r="8" fill="none" stroke="white" stroke-width="3" opacity="0.6"/>
    <circle cx="95" cy="10" r="8" fill="none" stroke="white" stroke-width="3" opacity="0.6"/>
    <rect x="20" y="0" width="10" height="25" fill="white" opacity="0.4"/>
    <rect x="90" y="0" width="10" height="25" fill="white" opacity="0.4"/>
    
    <!-- Calendar Grid -->
    <g opacity="0.4">
      <line x1="15" y1="80" x2="105" y2="80" stroke="white" stroke-width="1"/>
      <line x1="15" y1="100" x2="105" y2="100" stroke="white" stroke-width="1"/>
      <line x1="15" y1="120" x2="105" y2="120" stroke="white" stroke-width="1"/>
      <line x1="15" y1="140" x2="105" y2="140" stroke="white" stroke-width="1"/>
      
      <line x1="35" y1="65" x2="35" y2="155" stroke="white" stroke-width="1"/>
      <line x1="55" y1="65" x2="55" y2="155" stroke="white" stroke-width="1"/>
      <line x1="75" y1="65" x2="75" y2="155" stroke="white" stroke-width="1"/>
      <line x1="95" y1="65" x2="95" y2="155" stroke="white" stroke-width="1"/>
    </g>
    
    <!-- Highlighted Date -->
    <circle cx="45" cy="90" r="8" fill="#ffc107" opacity="0.8"/>
    <text x="45" y="95" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="white">15</text>
  </g>
  
  <!-- Event Symbols -->
  <g transform="translate(500, 120)">
    <!-- Podium -->
    <rect x="0" y="80" width="30" height="40" fill="white" opacity="0.3"/>
    <rect x="35" y="60" width="30" height="60" fill="white" opacity="0.4"/>
    <rect x="70" y="70" width="30" height="50" fill="white" opacity="0.35"/>
    
    <!-- Microphone -->
    <circle cx="50" cy="40" r="8" fill="white" opacity="0.5"/>
    <rect x="47" y="40" width="6" height="30" fill="white" opacity="0.4"/>
    <ellipse cx="50" cy="75" rx="15" ry="8" fill="white" opacity="0.3"/>
  </g>
  
  <!-- People Icons -->
  <g transform="translate(800, 150)">
    <!-- Person 1 -->
    <circle cx="0" cy="0" r="15" fill="white" opacity="0.4"/>
    <rect x="-12" y="15" width="24" height="35" rx="12" fill="white" opacity="0.4"/>
    
    <!-- Person 2 -->
    <circle cx="40" cy="0" r="15" fill="white" opacity="0.35"/>
    <rect x="28" y="15" width="24" height="35" rx="12" fill="white" opacity="0.35"/>
    
    <!-- Person 3 -->
    <circle cx="80" cy="0" r="15" fill="white" opacity="0.3"/>
    <rect x="68" y="15" width="24" height="35" rx="12" fill="white" opacity="0.3"/>
  </g>
  
  <!-- Conference Table -->
  <g transform="translate(600, 250)">
    <ellipse cx="0" cy="0" rx="100" ry="30" fill="white" opacity="0.2"/>
    <ellipse cx="0" cy="-5" rx="100" ry="30" fill="white" opacity="0.3"/>
    
    <!-- Chairs around table -->
    <g opacity="0.25">
      <rect x="-80" y="-40" width="15" height="20" rx="7" fill="white"/>
      <rect x="-40" y="-40" width="15" height="20" rx="7" fill="white"/>
      <rect x="0" y="-40" width="15" height="20" rx="7" fill="white"/>
      <rect x="40" y="-40" width="15" height="20" rx="7" fill="white"/>
      <rect x="80" y="-40" width="15" height="20" rx="7" fill="white"/>
      
      <rect x="-80" y="25" width="15" height="20" rx="7" fill="white"/>
      <rect x="-40" y="25" width="15" height="20" rx="7" fill="white"/>
      <rect x="0" y="25" width="15" height="20" rx="7" fill="white"/>
      <rect x="40" y="25" width="15" height="20" rx="7" fill="white"/>
      <rect x="80" y="25" width="15" height="20" rx="7" fill="white"/>
    </g>
  </g>
  
  <!-- Floating Elements -->
  <circle cx="150" cy="50" r="4" fill="white" opacity="0.3"/>
  <circle cx="1050" cy="80" r="5" fill="white" opacity="0.25"/>
  <circle cx="100" cy="350" r="3" fill="white" opacity="0.35"/>
  <circle cx="1100" cy="320" r="4" fill="white" opacity="0.3"/>
  
  <!-- Text Overlay -->
  <rect x="50" y="320" width="400" height="60" fill="rgba(0,0,0,0.4)" rx="8"/>
  <text x="70" y="345" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="white">
    Upcoming Events
  </text>
  <text x="70" y="365" font-family="Arial, sans-serif" font-size="14" fill="white" opacity="0.9">
    Join us for workshops, seminars, and community gatherings
  </text>
</svg>