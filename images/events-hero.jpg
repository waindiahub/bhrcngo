<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 400" width="1200" height="400">
  <defs>
    <linearGradient id="eventsGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#dc2626;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ef4444;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f87171;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="400" fill="url(#eventsGradient)"/>
  
  <!-- Calendar icon -->
  <g transform="translate(600,200)" fill="#ffffff" opacity="0.8">
    <!-- Calendar base -->
    <rect x="-80" y="-60" width="160" height="120" rx="10" fill="#ffffff" opacity="0.9"/>
    
    <!-- Calendar header -->
    <rect x="-80" y="-60" width="160" height="30" rx="10" fill="#dc2626"/>
    
    <!-- Calendar rings -->
    <rect x="-60" y="-80" width="8" height="40" rx="4" fill="#ffffff"/>
    <rect x="-20" y="-80" width="8" height="40" rx="4" fill="#ffffff"/>
    <rect x="20" y="-80" width="8" height="40" rx="4" fill="#ffffff"/>
    <rect x="60" y="-80" width="8" height="40" rx="4" fill="#ffffff"/>
    
    <!-- Calendar grid -->
    <g fill="#dc2626" opacity="0.7">
      <circle cx="-50" cy="-10" r="6"/>
      <circle cx="-20" cy="-10" r="6"/>
      <circle cx="10" cy="-10" r="6"/>
      <circle cx="40" cy="-10" r="6"/>
      
      <circle cx="-50" cy="15" r="6"/>
      <circle cx="-20" cy="15" r="6"/>
      <circle cx="10" cy="15" r="6"/>
      <circle cx="40" cy="15" r="6"/>
      
      <circle cx="-50" cy="40" r="8" fill="#dc2626"/>
      <circle cx="-20" cy="40" r="6"/>
      <circle cx="10" cy="40" r="6"/>
      <circle cx="40" cy="40" r="6"/>
    </g>
  </g>
  
  <!-- Event icons around -->
  <g fill="#ffffff" opacity="0.6">
    <!-- Microphone -->
    <g transform="translate(200,150)">
      <rect x="-5" y="-20" width="10" height="30" rx="5"/>
      <rect x="-15" y="15" width="30" height="3"/>
      <rect x="-2" y="18" width="4" height="15"/>
    </g>
    
    <!-- People group -->
    <g transform="translate(1000,150)">
      <circle cx="-15" cy="-10" r="8"/>
      <circle cx="0" cy="-10" r="8"/>
      <circle cx="15" cy="-10" r="8"/>
      <rect x="-23" y="0" width="16" height="20" rx="8"/>
      <rect x="-8" y="0" width="16" height="20" rx="8"/>
      <rect x="7" y="0" width="16" height="20" rx="8"/>
    </g>
    
    <!-- Megaphone -->
    <g transform="translate(150,300)">
      <path d="M-20,0 L20,-10 L25,-5 L25,5 L20,10 Z"/>
      <circle cx="25" cy="0" r="8"/>
      <rect x="-25" y="-3" width="10" height="6"/>
    </g>
    
    <!-- Award -->
    <g transform="translate(1050,300)">
      <circle cx="0" cy="-5" r="12"/>
      <path d="M-8,5 L0,15 L8,5 L0,0 Z"/>
      <circle cx="0" cy="-5" r="6" fill="#dc2626"/>
    </g>
  </g>
  
  <!-- Text -->
  <text x="600" y="80" text-anchor="middle" font-family="Arial, sans-serif" font-size="42" font-weight="bold" fill="#ffffff">
    Events & Activities
  </text>
  <text x="600" y="120" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" fill="#ffffff" opacity="0.9">
    Join Us in Making a Difference
  </text>
</svg>