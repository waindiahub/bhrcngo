<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 400" width="1200" height="400">
  <defs>
    <linearGradient id="galleryGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7c2d12;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ea580c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fb923c;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="400" fill="url(#galleryGradient)"/>
  
  <!-- Photo frames -->
  <g fill="#ffffff" opacity="0.8">
    <!-- Frame 1 -->
    <g transform="translate(300,200)">
      <rect x="-60" y="-45" width="120" height="90" fill="#ffffff" opacity="0.9"/>
      <rect x="-50" y="-35" width="100" height="70" fill="#f3f4f6"/>
      <!-- Photo placeholder -->
      <circle cx="0" cy="0" r="20" fill="#ea580c" opacity="0.6"/>
      <path d="M-15,-10 L15,-10 L10,10 L-10,10 Z" fill="#ea580c" opacity="0.4"/>
    </g>
    
    <!-- Frame 2 -->
    <g transform="translate(600,200) rotate(5)">
      <rect x="-60" y="-45" width="120" height="90" fill="#ffffff" opacity="0.9"/>
      <rect x="-50" y="-35" width="100" height="70" fill="#f3f4f6"/>
      <!-- Photo placeholder -->
      <rect x="-30" y="-20" width="60" height="40" fill="#ea580c" opacity="0.6"/>
      <circle cx="-15" cy="-10" r="5" fill="#ffffff"/>
    </g>
    
    <!-- Frame 3 -->
    <g transform="translate(900,200) rotate(-3)">
      <rect x="-60" y="-45" width="120" height="90" fill="#ffffff" opacity="0.9"/>
      <rect x="-50" y="-35" width="100" height="70" fill="#f3f4f6"/>
      <!-- Photo placeholder -->
      <path d="M-40,-25 L40,-25 L35,25 L-35,25 Z" fill="#ea580c" opacity="0.6"/>
      <circle cx="0" cy="0" r="8" fill="#ffffff"/>
    </g>
  </g>
  
  <!-- Camera icon -->
  <g transform="translate(150,150)" fill="#ffffff" opacity="0.7">
    <rect x="-25" y="-15" width="50" height="30" rx="5"/>
    <circle cx="0" cy="0" r="12"/>
    <circle cx="0" cy="0" r="8" fill="#ea580c"/>
    <rect x="-8" y="-25" width="16" height="10" rx="3"/>
    <circle cx="15" cy="-10" r="3"/>
  </g>
  
  <!-- Gallery grid icon -->
  <g transform="translate(1050,150)" fill="#ffffff" opacity="0.7">
    <rect x="-20" y="-20" width="15" height="15"/>
    <rect x="-20" y="-2" width="15" height="15"/>
    <rect x="-20" y="16" width="15" height="15"/>
    <rect x="5" y="-20" width="15" height="15"/>
    <rect x="5" y="-2" width="15" height="15"/>
    <rect x="5" y="16" width="15" height="15"/>
  </g>
  
  <!-- Decorative elements -->
  <g fill="#ffffff" opacity="0.4">
    <circle cx="100" cy="100" r="3"/>
    <circle cx="1100" cy="80" r="4"/>
    <circle cx="80" cy="320" r="2"/>
    <circle cx="1120" cy="300" r="3"/>
  </g>
  
  <!-- Text -->
  <text x="600" y="80" text-anchor="middle" font-family="Arial, sans-serif" font-size="42" font-weight="bold" fill="#ffffff">
    Photo Gallery
  </text>
  <text x="600" y="120" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" fill="#ffffff" opacity="0.9">
    Capturing Moments of Change and Hope
  </text>
</svg>